.share{
	padding-bottom:54px;
}
.padding3{
	padding:3% 3% 0;
}


.qgzlike h4{
	padding-top: 0.3rem;
	font-size:.28rem;
	text-align:center;
	padding-bottom: 0.24rem;
}
.qgzlike .item h6 {
	color: #9b9b9b;
	margin-top: .12rem;
	font-size: 0.20rem;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}

.qgzlike .item p {
	font-size: .3rem;
	color: #000;
	line-height: 1.3em;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
	margin-bottom: 5px;
	margin-top: 5px;
}

.qgzlike .item span {
	display: block;
	color: #d82315;
	padding-bottom: 0.36rem;
}

.qgzlike .item span i {
	color: #9b9b9b;
	text-decoration: line-through;
	margin-left: .2rem;
}

.qgzlike .likehat{
	position: relative;
	padding-top: 100%;
	overflow: hidden;
}
.qgzlike .likehat img{
	position: absolute;
	max-height: 100%;
	max-width: 100%;
	top: 0;
	bottom: 0;
	margin: auto;
	left: 0;
	right: 0;
}

/*瀑布流开始*/
.wall {
  display: block;
  position: relative;
  padding-bottom:54px;
}
.wall-column {
  display: block;
  position: relative;
  width: 50%;
  float: left;
  box-sizing: border-box;
  padding-bottom:54px;
}
.wall-column:nth-child(2n){
	padding-right: 3%;
	padding-left: 1.5%;
}
.wall-column:nth-child(2n+1){
	padding-left: 3%;
	padding-right: 1.5%;
}
/*瀑布流结束*/
.likehat{
	position:relative;
}
.likehat img{
	margin: auto;
	display: block;
}
.likehat .qblock{
	width:.8rem;
	height:.66rem;
	display:none;
	background:url(../images/dzhe.png) 0 0 no-repeat;
	background-size:100% 100%;
	top:0;
	left:0;
	position:absolute;
}
.likehat .qblock{
	display: block;
}
.likehat em{
	background:url(../images/sbg1.png) 0 0 no-repeat;
	background-size:100% 100%;
}
.likehat .sbg1{
	bottom:.1rem;
	right:.1rem;
	top:auto
}
.sbg1s{
	background:url(../images/sbg1s.png) 0 0 no-repeat!important;
	background-size:100% 100%!important;
}

